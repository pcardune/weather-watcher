{
  "name": "weather-watcher-webapp",
  "version": "0.0.1",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "webpack --config internals/webpack/webpack.prod.babel.js --profile --json > stats.json",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development babel-node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true babel-node server",
    "start:prod": "babel app -d ./es5/app && babel server -d ./es5/server && NODE_ENV=production node es5/server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy:prod": "npm run build && firebase use weather-watcher-170701 && firebase deploy",
    "deploy:prod-ci": "firebase use weather-watcher-170701 && firebase deploy --token=$FIREBASE_TOKEN --non-interactive",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "gitDir": "../",
    "*.js": [
      "prettier --write --single-quote --no-bracket-spacing --trailing-comma es5",
      "git add"
    ]
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css",
      "weather-watcher-core",
      "redux-firebase-mirror",
      "weather-watcher-cloud-functions"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!.*weather-watcher-core)/"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/*/RbGenerated*/*.{js,jsx}",
      "!app/app.js",
      "!app/routes.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      "^bundle-loader.*": "<rootDir>/internals/mocks/bundleModule.js",
      "^app/(.*)$": "<rootDir>/app/$1",
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "bundle-loader": "^0.5.5",
    "chalk": "1.1.3",
    "color": "^2.0.0",
    "compression": "1.6.2",
    "convert-units": "^2.2.0",
    "cross-env": "3.1.3",
    "express": "4.14.0",
    "firebase": "^4.1.2",
    "fontfaceobserver": "2.0.7",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "invariant": "2.2.2",
    "ip": "1.1.4",
    "leaflet": "^1.1.0",
    "localforage": "^1.5.0",
    "lodash": "4.17.2",
    "lodash.frompairs": "^4.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "minimist": "1.2.0",
    "moment-mini": "^2.18.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "randomcolor": "^0.5.3",
    "raven-js": "^3.17.0",
    "rc-slider": "^8.2.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-ga": "^2.2.0",
    "react-geosuggest": "^2.3.1",
    "react-helmet": "3.2.2",
    "react-intl": "2.1.5",
    "react-leaflet": "^1.3.1",
    "react-portal": "^3.1.0",
    "react-redux": "4.4.6",
    "react-responsive": "^1.3.4",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-truncate": "^2.1.4",
    "redux": "3.6.0",
    "redux-firebase-mirror": "^0.0.8",
    "redux-immutable": "3.0.8",
    "redux-persist": "^4.8.0",
    "redux-persist-immutable": "^4.3.0",
    "redux-saga": "^0.15.3",
    "redux-thunk": "^2.2.0",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "styled-components": "^2.1.1",
    "suncalc": "^1.8.0",
    "uuid": "^3.0.1",
    "victory": "^0.21.0",
    "warning": "3.0.0",
    "whatwg-fetch": "2.0.1"
  },
  "devDependencies": {
    "@storybook/cli": "^3.1.4",
    "@storybook/react": "^3.1.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-import-to-require": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-native": "^2.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "cheerio": "0.22.0",
    "circular-dependency-plugin": "2.0.0",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-redux-saga": "0.1.5",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "file-loader": "0.9.0",
    "firebase-tools": "^3.9.1",
    "flow-bin": "^0.49.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "jasmine-reporters": "^2.2.1",
    "jest-cli": "^20.0.4",
    "lint-staged": "^4.0.2",
    "ngrok": "2.2.4",
    "null-loader": "0.1.1",
    "offline-plugin": "4.5.2",
    "pre-commit": "1.1.3",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "15.4.1",
    "rimraf": "2.5.4",
    "shelljs": "0.7.5",
    "sinon": "^2.3.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-manifest-plugin": "^1.3.1"
  }
}
